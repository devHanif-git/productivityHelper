# Telegram Bot Configuration
TELEGRAM_TOKEN=your_bot_token_here

# Google Gemini API
# Single key (for backwards compatibility)
GEMINI_API_KEY=your_gemini_key_here

# Multiple keys for rotation (comma-separated, no spaces)
# The bot will automatically rotate to the next key when rate limited
GEMINI_API_KEYS=key1,key2,key3,key4,key5,key6,key7,key8,key9,key10

# Database
DATABASE_PATH=data/bot.db
